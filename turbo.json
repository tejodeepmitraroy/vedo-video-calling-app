{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env", ".env.*"],
      "outputs": [".next/**","dist/**", "!.next/cache/**"]
    },
     
    "lint": {},
  
   
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
  
}
