	"scripts": {
		"start": "node dist/index",
		"build": "tsc -p .",
		"dev": "tsc-watch --onSuccess \"node dist/index.js\"",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier . --check",
		"prettier:fix": "prettier . --write",
		"deploy": "docker build -t tejodeepmitraroy/vedo-video-call:v1 . && docker push tejodeepmitraroy/vedo-video-call:v1",
		"db:generate": "prisma generate",
		"db:studio": "prisma studio",
		"db:push": "prisma db push",
		"db:validate": "prisma db validate",
		"db:migrate": "npx prisma migrate dev --name init",
		"webhook": "ngrok http --domain swan-possible-dodo.ngrok-free.app 8000"
	},
	"devDependencies": {
		"@eslint/js": "^9.35.0",
